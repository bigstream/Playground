<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IEchoService" />
            </basicHttpBinding>
            <wsHttpBinding>
                <binding name="WSHttpBinding_IEchoService">
                    <security mode="Transport" />
                </binding>
            </wsHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:54891/EchoService/basichttp"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IEchoService"
                contract="EchoServiceReferenceHTTP.IEchoService" name="BasicHttpBinding_IEchoService" />
            <endpoint address="https://localhost:44316/EchoService/WSHttps"
                binding="wsHttpBinding" bindingConfiguration="WSHttpBinding_IEchoService"
                contract="EchoServiceReferenceHTTP.IEchoService" name="WSHttpBinding_IEchoService" />
        </client>
    </system.serviceModel>
</configuration>